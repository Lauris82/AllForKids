<!-- search modal -->
<div class="modal" tabindex="-1" role="dialog" aria-labelledby="search_modal" id="search_modal">
    <div class="widget widget_search">
        <form method="get" class="searchform form-inline" action="/">
            <div class="form-group">
                <input type="text" value="" name="search" class="form-control" placeholder="Search keyword" id="modal-search-input">
            </div>
            <button type="submit" class="theme_button">Search</button>
        </form>
    </div>
</div>


{% block topline %}
    <div class="page_topline">
        <div class="container-fluid">
            <div class="row">
                <div class="col-sm-4 ">
                    <div class="login-controls">
                        <div class="dropdown">
                            <a class="login-btn dropdown-toggle" data-toggle="dropdown">
                                <i class="rt-icon2-lock-open-outline"></i>Login
                            </a>
                            <div class="login-form-wrap">
                                <form class="form-horizontal dropdown-menu" method="post" action="/">
                                    <input type="text" class="form-control" id="login" placeholder="Login">
                                    <input type="password" class="form-control" id="pwd" placeholder="Password">
                                    <a href="#" class="forg-pass">Forgot Password?</a>
                                    <div class="checkbox">
                                        <input type="checkbox" id="checkbox"><label for="checkbox"> Remember</label>
                                    </div>
                                    <button type="submit" id="login_form_submit" name="login_submit" class="btn-default btn-type1">Login</button>
                                </form>
                            </div>
                        </div>
                        <a href="#" class="register-btn dropdown-toggle" data-toggle="dropdown">
                            <i class="rt-icon2-user-outline"></i>Register
                        </a>
                    </div>
                </div>
                <div class="col-sm-4 text-center second-column">
                    <div class="widget widget_search">
                        <form method="get" class="searchform form-inline" action="/">
                            <div class="form-group">
                                <label class="screen-reader-text" for="page_search">Search for:</label>
                                <input id="page_search" type="text" value="" name="search" class="form-control" placeholder="Type Search Keyword here">
                            </div>
                            <button type="submit" class="rt-icon2-zoom-outline">Search</button>
                        </form>
                    </div>
                </div>
                <div class="col-sm-4 text-right third-column">
                    <div class="address">
                        <i class="rt-icon2-location-outline"></i> 23 Harper St. Owensboro
                    </div>
                </div>
            </div>
        </div>
    </div>
{% endblock topline %}



